{
  "name": "modern-gekko",
  "version": "1.0.0",
  "description": "Modern fork of askmike/gekko - Trading bot and backtester with Vue 3 UI",
  "main": "src/server.js",
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "concurrently \"nodemon src/server.js\" \"npm run web:dev\"",
    "web:dev": "cd web && npm run dev",
    "web:build": "cd web && npm install && npm run build",
    "web:preview": "cd web && npm run preview",
    "build": "npm run web:build",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.config.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ test/ --ext .js --fix && cd web && npm run lint",
    "lint:check": "eslint src/ test/ --ext .js && cd web && npm run lint",
    "docker:build": "docker build -t modern-gekko .",
    "docker:run": "docker run -p 3000:3000 modern-gekko",
    "docker:compose": "docker-compose up --build"

  },
  "keywords": [
    "trading",
    "bot",
    "backtesting",
    "cryptocurrency",
    "gekko",
    "vue",
    "nodejs"
  ],
  "author": "Modern Gekko Team",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "helmet": "^8.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "multer": "^2.0.2",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.0",
    "winston": "^3.17.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3"
  }
}
